CREATE DATABASE hello
use hello





SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[question](
	[ID] [int] IDENTITY(1,1) NOT NULL,
	[QUESTION] [nvarchar](500) NOT NULL,
	[A] [nvarchar](250) NOT NULL,
	[B] [nvarchar](250) NOT NULL,
	[C] [nvarchar](250) NOT NULL,
	[D] [nvarchar](250) NOT NULL,
	[answer] [nvarchar](250) NOT NULL
) ON [PRIMARY]
GO
ALTER TABLE [dbo].[question] ADD PRIMARY KEY CLUSTERED 
(
	[ID] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, SORT_IN_TEMPDB = OFF, IGNORE_DUP_KEY = OFF, ONLINE = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
GO


SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[record](
	[name] [nvarchar](30) NULL,
	[testdate] [date] NULL,
	[result] [nvarchar](30) NULL
) ON [PRIMARY]
GO


SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[human](
	[ID] [int] IDENTITY(1,1) NOT NULL,
	[NAME] [nvarchar](30) NOT NULL,
	[PASSWORD] [nvarchar](30) NULL
) ON [PRIMARY]
GO
SET ANSI_PADDING ON
GO
ALTER TABLE [dbo].[human] ADD  CONSTRAINT [human_pk] PRIMARY KEY NONCLUSTERED 
(
	[ID] ASC,
	[NAME] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, SORT_IN_TEMPDB = OFF, IGNORE_DUP_KEY = OFF, ONLINE = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
GO

